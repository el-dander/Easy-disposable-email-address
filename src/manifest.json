{
   "manifest_version": 2,
   "background": {
     "scripts": [ "providers.js", "background.js" ],
     "persistent": false
   },
   "description": "Simply right click where you need a disposable email address, and your temporary inbox will appear in the next tab.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "name": "Easy disposable email address",
   "options_page": "options.html",
   "permissions": [ "contextMenus", "tabs", "http://*/*", "https://*/*" ],
   "version": "1.3"
}
